<div class="container">
  <h1 style="width: 100%">Pagina {{page}}</h1>
  {{#each products}}
    
      <div class="tarjetas">
        <a href="/api/products/{{this.id}}">
            <p>{{this.title}}</p>
            <p>{{this.description}}</p>
            <p>{{this.code}}</p>
            <p>${{this.price}}</p>
            <p>{{this.stock}}</p>
            <p>{{this.category}}</p>
            <p>{{this.thumbnail}}</p>
        </a>
      </div>
    
  {{/each}}
</div>